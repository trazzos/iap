    <access origin="*" />
	<allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#000000" />
    </platform>
    <gap:config-file platform="ios" parent="NSPhotoLibraryUsageDescription">
        <string>User can opt to upload a photo or video as a response of an activity</string>
    </gap:config-file>
    <gap:config-file platform="ios" parent="NSCameraUsageDescription">
        <string>User can opt to take a photo or record a video as a response of an activity</string>
    </gap:config-file>
